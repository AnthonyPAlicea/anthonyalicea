<div>
    <div>
        <div>
            {% if pagination.href.previous %}
                <div>
                    <a href="{{ pagination.href.previous | url }}">Previous Page</a>
                </div>
            {% endif %}

            {% if pagination.href.next %}
                <div>
                    <a href="{{ pagination.href.next | url }}">Next Page</a>
                </div>
            {% endif %}
        </div>

        <div>
            {% if page.url !== pagination.href.first %}
                <div>
                    <a href="{{ pagination.href.first | url }}">First Page</a>
                </div>

            {% endif %}

            {% if page.url !== pagination.href.last %}
                <div>
                    <a href="{{ pagination.href.last | url }}">Last Page</a>
                </div>
            {% endif %}
        </div>
    </div>

    <p>Page
        {{ pagination.pageNumber + 1 }}
        of
        {{ pagination.pages.length }}</p>
</div>